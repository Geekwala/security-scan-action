{
  "name": "geekwala-security-scan-action",
  "version": "1.1.0",
  "description": "GitHub Action to scan dependencies for vulnerabilities using GeekWala API",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "github-action",
    "security",
    "vulnerability-scanning",
    "dependencies",
    "geekwala"
  ],
  "author": "GeekWala",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.3",
    "@actions/github": "^9.0.0",
    "axios": "^1.6.5",
    "js-yaml": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^10.0.0",
    "eslint-plugin-jest": "^29.13.0",
    "jest": "^30.2.0",
    "nock": "^14.0.11",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
